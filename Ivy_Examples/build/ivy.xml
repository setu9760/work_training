		<dependency org="org.apache.logging.log4j" name="log4j-api"
			rev="2.1" />
		
		<dependency org="org.apache.logging.log4j" name="log4j-core"
			rev="2.1"  />
	-->
		<dependency org="mysql" name="mysql-connector-java" rev="5.1.33" />
		
		<dependency org="junit" name="junit" rev="4.11" conf="test->default" />
		<!--
		<dependency org="cglib" name="cglib" rev="3.1"/>
		-->
		<dependency org="org.springframework" name="spring-jdbc" rev="4.1.0.RELEASE" 
		transitive="true"/>
		
		<dependency org="org.springframework" name="spring-context" rev="4.1.0.RELEASE" 
		transitive="true"/>
		
		<dependency org="org.springframework" name="spring-test" rev="4.1.1.RELEASE" 
		transitive="false"/>
		
		<dependency org="org.springframework" name="spring-core" rev="4.1.0.RELEASE"
		transitive="false"/>
		
		<dependency org="org.springframework" name="spring-context-support" rev="4.1.0.RELEASE"
