<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-4.1.xsd">

	<context:annotation-config />

	<bean
		class="org.springframework.beans.factory.annotation.AutowiredAnnotationBeanPostProcessor" />

	<bean id="CustomerBean" class="com.desai.common.autowiring.Customer">
		<property name="num" value="2" />
	</bean>

	<bean id="PersonBean1" class="com.desai.common.autowiring.Person">
		<property name="name" value="mkyong1" />
		<property name="age" value="1" />
	</bean>

	<bean id="PersonBean2" class="com.desai.common.autowiring.Person">
		<property name="name" value="mkyong2" />
		<property name="age" value="2" />
	</bean>

	<bean id="PersonBean3" class="com.desai.common.autowiring.Person">
		<property name="name" value="mkyong3" />
		<property name="age" value="3" />
	</bean>

	<bean id="PersonBean4" class="com.desai.common.autowiring.Person">
		<property name="name" value="mkyong4" />
		<property name="age" value="4" />
	</bean>

</beans>