<!-- Spring-Common.xml -->
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="OutputHelper" class="com.desai.common.OutputHelper">
		<property name="iOutputGenerator">
			<ref bean="JsonOutputGenerator" />
		</property>
	</bean>

	<bean id="CsvOutputGenerator" class="com.desai.common.CsvOutputGenerator" />
	<bean id="JsonOutputGenerator" class="com.desai.common.JsonOutputGenerator" />
</beans>